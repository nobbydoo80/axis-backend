{% verbatim %}
    <table class="table table-striped table-bordered" id="annotation_tbl_{{id}}">
        <thead>
            <tr>
                <th>Date</th>
                <th>User</th>
                <th>Annotation</th>
            </tr>
        </thead>
        {{#each annotations}}
            <tr>
                <td>{{created_on}}</td>
                <td>{{user}}</td>
                <td>{{content}}</td>
            </tr>
        {{/each}}

    </table>
{% endverbatim %}
